.page-register{
  margin-top: 1px;
  padding-bottom: 0.2rem;
  .guide-center {
    p {
      font-family: PingFangSC-Regular;
      font-size: 0.22rem;
      text-align: center;
      line-height: 0.22rem;
      margin-top: 0.24rem;
    }
    .guide-text {
      margin-top: 0.12rem;
      font-size: 0.14rem;
      text-align: center;
    }
    .guide-ht {
      vertical-align: middle;
    }
    .goRegister {
      vertical-align: middle;
      font-size: 0.14rem;
    }
  }
  // 主体
  .page-register-content {
    padding-top: 40px; // 90 - 44
    .content-center {
      width: 100%;
      padding: 0 0.15rem;
      box-sizing: border-box;
      margin: 0 auto;
      .userNone {
        li {
          cursor no-drop!important
        }
      }
      .registerTab {
        margin-bottom: 24px;
      }
      // 忘记密码
      .resgister-opions {
        font-size: 12px;
        margin-top: 8px;
        .common-checkout {
          vertical-align: middle;
        }
        .text {
          user-select none;
          display: inline-block;
          vertical-align: middle;
          margin-left: 12px;
          line-height: 16px;
          cursor: pointer;
          height: 14px;
          margin-right: 4px;
        }
        a {
          vertical-align: middle;
          line-height: 16px;
          font-size: 12px;
          height: 14px;
          margin-left: 4px
        }
      }
    }
  }
  .guide-center{
    .logBtn{
      .common-button-slot{
        color #1baaec !important
      }
    }
  }
  .page-login-content{
    .logBtn{
      .common-button-slot{
        color #1baaec !important
      }
    }
  }
  .copy {
    display: none;
  }
}
// 返回首页按钮
.go-home-btn {
  border-width: 1px;
  border-style: solid;
  height: 30px;
  line-height: 30px;
  width: 110px;
  text-align: center;
  cursor: pointer;
  position: absolute;
  top: 10px;
  right: 20px;
  z-index: 10;
  user-select: none;
  display: none;
}