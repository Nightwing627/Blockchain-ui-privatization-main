    padding: 0 0.15rem;
    box-sizing: border-box;
    line-height: 0.4rem;
    margin-top: 1px;
  }
  .center-inputs{
    // width:380px;
    padding: 0 0.15rem;
    margin-top: 0.2rem;
  }
  .center-code{

    img{
      width: 100px;
      height: 100px;
      margin-bottom 30px
    }
    div{
      height:70px;
      p:nth-of-type(1){
        font-size: 12px;
        padding: 12px 0 0 20px;
      }
      p:nth-of-type(2){
