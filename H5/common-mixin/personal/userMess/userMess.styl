.user-management-header {
  width: 100%;
  float: left;
  position: relative;
  margin: 1px 0;
}

.user-management-header-img {
  float: left;
  width: 75px;
  height: 75px;
  margin: 18px 0 17px 20px;
}

.user-management-header-img2 {
  float: right;
  height: 122px;
  position: absolute;
  right: 0;
  bottom: 0;
}

.user-management-header-message {
  float: left;
  margin: 15px 0 0 10px;
  font-size: 12px;

  p {
    button {
      margin-left: 10px;
    }
  }
}

.user-management-header-message p:nth-of-type(1) span {
  line-height: 24px;
}

.user-management-header-message span:nth-of-type(2) {
  margin-left: 10px;
  cursor: pointer;
}

.user-management-header-message p:nth-of-type(2) {
  margin: 5px 0 10px 0;
  font-size: 22px;
  font-weight: normal;
}