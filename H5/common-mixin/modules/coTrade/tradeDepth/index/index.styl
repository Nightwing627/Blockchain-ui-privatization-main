#co-tradeDepth {
  padding-left: 0.08rem;
  .td-symbol1 {
    white-space: nowrap;
  }
  .td-rose {
    display: none!important;
  }
  .td-price, .td-symbol, .td-symbol1, .td-symbol0 {
    width: 50%!important;
    box-sizing: border-box;
  }
  .td-symbol, .td-symbol0 {
    padding-left: 0px!important;
  }
  .td-symbol1 {
    padding-right: 0.15rem;
  }
  .symbol-item {
    line-height: 22px!important;
    height: 22px;
  }
  .tr-bg {
    height: 0.22rem!important;
    bottom: 0!important;
    top: -0.005rem!important;
    right: 0!important;
    // top: -1.5px!important;
    left: initial!important;
  }
}
.depthSelect {
  .selectList {
    right: 0;
  }
}

  .coDepth {
    width: 100%;
    position: relative;
    overflow: hidden;
  }
  .block-header {
    height: 39px;
    line-height: 38px;
    padding: 0 15px;
    font-size: 14px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
  }
  .tbody {
    padding: 4px 0;
  }
  .current-price {
    height: 0.4rem;
    line-height: 0.4rem;
    // padding: 0 0.15rem;
    font-size: 12px;
    margin-top: -0.05rem;
    .price-box {
      float: left;
      height: 40px;
      line-height: 40px;
      font-size: 12px;
      overflow: hidden;
      span {
        vertical-align: middle;
      }
      .price {
        margin-right: 10px;
      }
    }
  }
  .jb {
    margin-right: 0.15rem;
    display: inline-block;
    float: right;
    i {
      vertical-align: middle;
      display: inline-block;
      width: 6px;
      margin-left: 3px;
      height: 12px;
      border-width: 1px;
      border-style: solid;
      border-radius: 2px;
    }
  }
  .switch-btn {
    float: right;
    .dtIcon {
      font-size: 16px;
      margin-left: 5px;
      cursor: pointer;
      display: inline-block;
      height: 36px;
      line-height: 36px;
      text-align: center;
      border-radius: 50%;
      width: 36px;
    }
  }
  .h4 {
    display: inlin;
  }
  .title-tab {
    margin-right: 30px;
    display: none;
    &.title-new-tab {
      display: none;
    }
  }
  .trade-shrinks {
    .h4 {
      display: none;
    }
    .title-tab {
      padding: 0 3px;
      cursor: pointer;
      display: inline-block;
      &.title-new-tab {
        display: inline-block;
      }
      &:hover, &.selected {
        border-bottom-style: solid;
        border-bottom-width: 2px;
      }
    }
    .new-tradb, .block-content {
        position: absolute;
        top: 40px;
        left: 0;
        transform: translate(-100%, 0px);
        // left: -100%;
        transition: transform 0.35s cubic-bezier(0.15, 0.3, 0.25, 1) 0s;
        will-change: transform;
      &.is-show {
        display: block;
        left: 0;
        transform: translate(0px, 0px);
      }
    }
    .new-tradb {
      // left: 100%;
      transform: translate(100%, 0px);
    }
  }
  // 国际版
  .Int-contract-content {
    .coDepth,
    .coDepth-block {
      height: 100%;
    }
    .block-content,
    .new-tradb {
      height: calc(100% - 40px);
    }
  }