.innovation-list {
  // padding-bottom: 0.5rem;
  .loading-box {
    // margin-left: 50%;
    margin-top: 0.8rem;
    // transform: translateX(-50%);
    // padding: 80px 0 80px;
    text-align: center;
  }
  .ieo-management-header{
    height: 40px;
    margin-top: 1px;
    position: relative;
    padding: 0 15px;
  }
  .page-banner {
    height: 220px
    .title {
      margin-top: 110px;
      font-size: 32px;
      line-height: 32px;
    }
  }
  .innovation-content {
    // width: 1180px;
    margin: 0 auto;
    // padding-top: 20px;
    border-radius: 2px;
    .innovation-list {
      clear: both;
      padding-top: 10px;
    }
    .message {
      .img {
        width: 0.4rem;
        height: 0.4rem;
        float: left;
        img {
          width: 100%;
          height: 100%;
        }
      }
      .message-titie {
        float: left;
        font-size: 0.18rem;
        line-height: 0.22rem;
        font-weight: normal;
        .message-text {
          margin-left: 0.15rem;
          width: 2.3rem;
          font-size: 0.14rem;
          display: block;
          word-wrap: break-word;word-break: break-all;overflow: hidden;
        }
        .statusLabel {
          font-size: 12px;
          border-style: solid;
          border-width: 1px;
          display: inline-block;
          height: 22px;
          line-height: 22px;
          padding: 0 8px;
          border-radius: 0px 6px 0px 6px;
          vertical-align: middle;
          margin-top: 0.1rem;
          margin-left: 0.15rem;
        }
      }
    }
    .innovation-item {
      padding: 0.2rem 0.15rem;
      // margin-top: 20px;
      border-radius: 2px;

      .info {
        // padding-left: 100px;
        .tit {
          font-size: 22px;
          line-height: 22px;
          font-weight: normal;
          .statusLabel {
            font-size: 12px;
            border-style: solid;
            border-width: 1px;
            display: inline-block;
            height: 22px;
            line-height: 22px;
            padding: 0 8px;
            border-radius: 0px 6px 0px 6px;
            vertical-align: middle;
            margin-left: 26px;
          }
        }
        .bf {
          font-size: 12px;
          // width: 320px;
          // float: left;
          margin: 14px 0 25px;
          p {
            line-height: 12px;
          }
          .jdt {
            position relative
            height 6px
            width 100%
            margin 9px 0px
            overflow hidden
          }
          .jdt-bar {
            position absolute
            top 0
            left 0
            height 100%
            width 50%
          }
          .num {
            line-height: 14px;
          }
        }
        .cz {
          // float: right;
          .timer {
            // float: left;
            // margin: 10px 18px 0 0;
            font-size: 14px;
            span {
              vertical-align: middle;
            }
            .tim {
              font-size: 12px;
              margin-right: 10px;
            }
            .val {
              margin-top: 0.1rem;
            }
          }
          .common-button {
            margin: 0.2rem 0 0.3rem 0!important;
            // float: right;
          }
        }
        .text {
          clear: both;
          font-size: 12px;
          line-height: 18px;
          // padding: 16px 20px 14px 0;
          border-top-width: 1px;
          border-top-style: solid;
        }
      }
    }
  }
  .f-r {
    float: right;
  }
  .no-data {
    // padding: 80px 0 80px;
    clear: both;
    text-align: center;
    font-size: 12px;
    p {
      margin-top: 5px;
    }
    .icon {
      font-size: 36px;
    }
  }
}
