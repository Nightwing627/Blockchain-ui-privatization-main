.home-table {
  width: 100%;
  font-size: 12px;
  .home-loading {
    height: 300px;
    position: relative;
    .loading-page {
      position: absolute;
      left: 50%;
      top: 50%;
      transform: translate3d(-50%, -50%, 0)
    }
  }
  .home-nodata {
    height: 300px;
    position: relative;
    padding: 80px 0 80px;
    text-align: center;
    font-size: 12px;
    box-sizing: border-box;
    p {
      margin-top: 5px;
    }
    .icon {
      font-size: 36px;
    }
  }
  .home-thead {
    user-select: none;
    width: 100%;
    font-size: 12px;
    height: 30px;
    padding: 0 15px;
    box-sizing: border-box;
    border-bottom-style: solid;;
    border-bottom-width: 1px
    .sortable {
      cursor: pointer
    }
    li {
      height: 30px;
      line-height: 30px;
      text-align: right;
      float: left
      &:nth-child(1) { width: 33% }
      &:nth-child(2) { width: 33% }
      &:nth-child(3) { width: 33% }
      &:nth-child(4) { width: 15% }
      &:nth-child(5) { width: 15% }
      &:nth-child(6) { width: 15% }
      &:nth-child(7) { width: 15% }
    }
    li:first-child {
      text-align: left;
    }
  }
  .home-tbody-box {
    padding-bottom: 0.2rem;
    transition: all 0.5s;
    // min-height: 2rem;
  }
  .home-tbody {
    .home-tbody-title {
      height: 55px;
      line-height: 55px;
      .lable {
        border-left-width: 3px;
        border-left-style: solid;
        padding-left: 10px;
        margin-left: 1px;
      }
    }
    .home-tbody-list {
      .home-tbody-li {
        height: 55px;
        padding: 0 20px;
        .even {
          line-height: 55px;
          height: 55px;
          float: left;
          text-align: right;
          &:nth-child(1) { width: 33% }
          &:nth-child(2) { width: 33% }
          &:nth-child(3) { width: 33% }
          &:nth-child(4) { width: 15% }
          &:nth-child(5) { width: 15% }
          &:nth-child(6) { width: 15% }
          &:nth-child(7) { width: 15% }
          .rose-label {
            display: inline-block;
            width: 70px;
            height: 34px;
            text-align: center;
            line-height: 34px;
            border-radius: 2px;
            padding: 0;
          }
          .evenSymbol {
            margin-left:6px;
            font-weight: 700!important;
            cursor: pointer;
            font-size: 14px;
            position relative
            .coin-label {
              white-space nowrap;
              position:absolute;
              right:-5px;
              transform:translateX(100%);
              top:-4px;
              white-space:nowrap;
              display:inline-block;
              font-size:12px;
              font-weight:400;
              padding:0px 4px;
              border-radius:2px;
              line-height:17px;
              overflow: hidden;
              .coin-label {
                position: relative;
                z-index: 1;
                margin: 0;
                padding: 0;
              }
              .coin-bg {
                display: inline-block;
                width: 100%;
                height: 100%;
                position: absolute;
                left: 0;
                top: 0;
                opacity: 0.2;
              }
            }
          }
          .marketIcon {
            cursor: pointer
          }
        }
        .newPrice {
          line-height: auto !important;
          padding-top: 12px;
          box-sizing: border-box;
          p {
            line-height: 15px;
          }
          .price {
            font-size: 14px;
            margin-bottom: 4px;
          }
        }
        .even:first-child {
          text-align: left;
        }
      }
    }
  }
}
.tableContenth5 {
  white-space: nowrap;
  width: 100%;
  margin: 10px auto 0 auto;
  position: relative;
  box-shadow: 0px -2px 10px rgba(0,0,0,0.2)
  .common-table {
    background: none;
  }
  .market-title-box {
    // padding: 0 0.15rem;
  }
  .market-title-scroll {
    // width: 100%;
    height: 0.4rem;
    overflow-y: scroll;
    padding: 0 0.15rem;
  }
  .market-title {
    width: auto!important;
    // padding: 0 0.15rem;
    // margin: 0 -0.15rem;
    // width: 100%;
    // border-bottom-width: 1px;
    // border-bottom-style: solid;
    // height: 0.4rem;
    // line-height: 0.4rem;
    // border-radius: 2px 2px 0 0;
    display: flex;
    padding-right: 0.15rem;
    // display: flex;
    li {
      height: 0.38rem;
      line-height: 0.4rem;
      text-align: center;
      // min-width: 0.5rem;
      white-space:nowrap
      padding: 0 0.1rem;
      &.selected {
        border-bottom-width: 2px;
        border-bottom-style: solid;
      }
    }
  }
  .search-box {
    padding: 0 0.15rem;
  }
  .search {
    height: 0.28rem;
    line-height: 0.26rem;
    width: 100%;
    border-radius: 2px;
    border-width: 1px;
    border-style: solid;
    margin: 0.1rem 0;
    .icon-serach {
      margin: 0 10px;
    }
    .search-input {
      line-height: 0.28rem;
      border: none;
      width: 86%;
    }
    input::-webkit-input-placeholder {
      color: #515A6B;
      font-size: 0.12rem;
    }
  }

  .table-body td .rose-label {
    display: inline-block;
    width: 70px;
    height: 24px;
    text-align: center;
    line-height: 24px;
    padding: 0;
  }
  .table-body td .symbolName ,
  .table-body td .icon-btn {
    padding: 0;
    vertical-align: inherit;
    margin: 0;
    display: inline-block;
  }
  .table-body td .symbolName {
    width: 80%;
    margin-left: 10px;
  }
  .fontSize14 {
    font-size: 0.14rem;
  }
}
