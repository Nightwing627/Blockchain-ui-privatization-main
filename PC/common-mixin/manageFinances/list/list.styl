.managefinances {
  .page-banner {
    height: 220px
    .title {
      margin-top: 110px;
      font-size: 32px;
      line-height: 32px;
    }
  }
  .managefinances-content {
    width: 1180px;
    margin: 0 auto;
    padding-top: 40px;
    border-radius: 2px;
    .manageTop{
      height:54px;
      .topLeft{
        float:left;
        .topTitle{
          font-size:16px;
          margin-right:10px;
        }
        .topNotice{
          font-size:12px;
        }
      }
      .li-right{
        float: right;
        .record{
          font-size:14px;
        }
      }
    }
    .manageList{
      .manageListItem{
        display:inline-block;
        height : 258px;
        width : 320px;
        padding : 30px;
        border-radius : 2px;
        margin-right:20px;
        .itemCon1{
          font-size: 16px;
          text-align : center;
          margin-bottom: 8px;
        }
        .itemCon2{
          text-align : center;
          margin-bottom: 20px;
          font-size: 12px;
        }
        .itemCon3{
          text-align:center;
          font-size:42px;
          margin-bottom: 12px;
          .scale{
            font-size:22px;
            margin-left:8px;
          }
        }
        .itemCon4{
          font-size: 12px;
          text-align: center;
          margin-bottom: 30px;
        }
        .itemCon5{
          width: 210px;
          margin: 0 auto;
          text-align: center;
          margin-bottom: 24px;
          overflow: hidden;
          .itemCon5Left{
            float: left;
            width: 104px;
            border-right-width: 1px;
            border-right-style: solid;
          }
          .itemCon5right{
            float:left;
            width:105px;
          }
          .subItemTop{
            font-size:16px;
            margin-bottom:6px;
          }
          .subItemBot{
            font-size:12px;
          }
        }
        .itemCon6{
          clear:both;
          width:210px;
          margin: 0 auto;
          .bottom-button{
            width:100%
          }
        }
      }
      .manageListItem:last-child{
        margin-right:0px;
      }
    }
  }
  .f-r {
    float: right;
  }
  .no-data {
    padding: 80px 0 80px;
    clear: both;
    text-align: center;
    font-size: 12px;
    p {
      margin-top: 5px;
    }
    .icon {
      font-size: 36px;
    }
  }
}