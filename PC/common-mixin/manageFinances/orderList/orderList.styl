.orderList{
  .page-banner {
    height: 220px
    .title {
      margin-top: 110px;
      font-size: 32px;
      line-height: 32px;
    }
  }
  .orderListCon{
    width:1180px;
    margin:40px auto;
    .orderListTop{
      height:54px;
      font-size: 14px;
      line-height 54px;
      padding-left:20px;
      border-bottom-style:solid;
      border-bottom-width:1px;
    }
    .orderListCon-options{
      width: 100%;
      box-sizing: border-box;
      padding: 14px 20px 0px;
      .common-select {
        display: inline-block;
        margin-right: 30px;
      }
    }
    .orderListCon-table{
      width: 100%;
      box-sizing: border-box;
    }
  }
}