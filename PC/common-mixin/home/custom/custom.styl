.europeHome-custom {
  padding-bottom: 100px;
  margin-top: -90px;
  .custom-bg {
    padding-top: 192px;
    background: linear-gradient(#F4F7FB , #fff); /* 标准的语法 */
  }
  .custom-title {
    text-align: center;
    font-size: 36px;
    color: #31363E;
    font-weight: 900;
  }
  .custom-maker {
    text-align: center;
    font-size: 16px;
    color: #828EA1;
    margin-top: 23px;
  }
  .product-inof {
    width: 1180px;
    margin: 0 auto;
    padding-bottom: 125px;
    .pro-img {
      width: 750px
      float: left;
      margin-left: -20px;
      margin-top: 5px;
      position: relative;
      .dh {
        position: absolute;
        top: 0;
      }
    }
    .pro-text {
      width: 400px;
      float: right;
      margin-top: 60px;
      .info-item {
        .info-icon {
          float: left;
          margin-right: 22px;
        }
        .info-text {
          width: 340px;
          float: left;
          margin-bottom: 28px;
          .tit {
            font-size: 16px;
            color: #5A6679;
            font-weight: 900;
          }
          .txt {
            font-size: 14px;
            color: #828EA1;
            margin-top: 10px;
            line-height: 20px;
          }
        }
      }
    }
    .down-block {
      .down-btn {
        width: 160px;
        height: 40px;
        float: left;
        cursor: pointer;
        margin:6px 8px 0 0;
        position: relative;
        img {
          width: 100%;
        }
        &.down-btn-2 {
          margin-right: 0;
        }
        &:hover {
          .hover-bg {
            background: rgba(255,255,255,0.15);
            position: absolute;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
          }
        }
      }
      .ewm {
        float: left;
        width: 64px;
        height: 64px;
        position: relative;
        cursor: pointer;
        .bug-ewm {
          position: absolute;
          background: #fff;
          border: 0 solid #DDDDDD;
          bottom: 70px;
          right: 11px;
          width: 120px;
          height: 150px;
          padding: 15px;
          box-shadow: 0 2px 10px 0 rgba(0,0,0,0.10);
          border-radius: 4px;
          display: none;
          p {
            font-size: 12px;
            color: #828EA1;
            margin-top: 10px;
          }
          .ewm-img {
            border: 1px solid #D5DEE7;
            border-radius: 4px;
            padding: 1px;
            display: block;
            img {
              width: 100%;
              height: 100%;
            }
          }
        }
        .hove-show {
          display: none;
        }
        &:hover {
          .bug-ewm {
            display: block;
          }
          .hove-hide {
            display: none;
          }
          .hove-show {
            display: block;
          }
        }
      }
    }
  }
  .product-ser-box {
    background: #fff;
  }
  .product-ser {
    width: 1180px;
    margin: 0 auto;
    padding-top: 1px;
    .img {
      width: 383px;
      float: right;
    }
    .text {
      width: 700px;
      float: left;
      margin-top: 100px;
      .title {
        font-size: 36px;
        color: #31363E;
      }
      .maker {
        width: 450px;
        margin-top: 23px;
        font-size: 16px;
        color: #828EA1;
        line-height: 22px;
      }
    }
    .label-block {
      margin-top: 30px;
      .label-item {
        width: 278px;
        height: 68px;
        line-height: 69px;
        text-align: center;
        border: 1px solid #D5DEE7;
        border-radius: 4px;
        float: left
        margin-right: 20px;
        img {
          vertical-align: middle;
          margin-right: 20px;

        }
        &:hover {
          box-shadow: 0 4px 12px 0 rgba(0,0,0,0.10);
        }
      }
      .last-item {
        margin: 0;
      }
    }
  }
}
