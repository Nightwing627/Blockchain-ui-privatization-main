.interHome-custom {
  .navsBase {
    padding-top: 120px;
    height: 650px;
    .title {
      text-align: center;
      font-size: 36px;
      max-width: 1180px;
      margin: 0 auto;
    }
    .mess {
      text-align: center;
      font-size: 16px;
      max-width: 1180px;
      margin: 0 auto;
      margin-top: 25px;
    }
    .custom {
      margin: 0 auto;
      width: 1120px;
      margin-top: 100px;
    }
    .custom-even-base {
      height: 150px;
      width: 50%;
      float: left;
      margin-bottom: 39px;
