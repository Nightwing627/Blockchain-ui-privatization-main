.home-table {
  min-width:1100px;
  width: 100%;
  font-size: 12px;
  .home-loading {
    height: 300px;
    position: relative;
    .loading-page {
      position: absolute;
      left: 50%;
      top: 50%;
      transform: translate3d(-50%, -50%, 0)
    }
  }
  .home-nodata {
    height: 300px;
    position: relative;
    padding: 80px 0 80px;
    text-align: center;
    font-size: 12px;
    box-sizing: border-box;
    p {
      margin-top: 5px;
    }
    .icon {
      font-size: 36px;
    }
  }
  .home-thead {
    user-select: none;
    width: 100%;
    font-size: 12px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    height: 30px;
    padding: 0 20px;
    box-sizing: border-box;
    .sortable {
      cursor: pointer
    }
    li {
      height: 30px;
      line-height: 30px;
      text-align: right;
      float: left
      &:nth-child(1) { width: 12% }
      &:nth-child(2) { width: 15% }
      &:nth-child(3) { width: 13% }
      &:nth-child(4) { width: 15% }
      &:nth-child(5) { width: 15% }
      &:nth-child(6) { width: 15% }
      &:nth-child(7) { width: 15% }
    }
    li:first-child {
      text-align: left;
    }
  }
  .home-tbody-box {
    transition: all 0.5s
  }
  .home-tbody {
    .home-tbody-title {
      height: 55px;
      border-bottom-width: 1px;
      border-bottom-style: solid;
      line-height: 55px;
      .lable {
        border-left-width: 3px;
        border-left-style: solid;
        padding-left: 10px;
        margin-left: 1px;
      }
    }
    .home-tbody-list {
      .home-tbody-li {
        height: 55px;
        border-bottom-width: 1px;
        border-bottom-style: solid;
        padding: 0 20px;
        overflow: hidden;
        .even {
          line-height: 55px;
          float: left;
          text-align: right;
          &:nth-child(1) { width: 12% }
          &:nth-child(2) { width: 15% }
          &:nth-child(3) { width: 13% }
          &:nth-child(4) { width: 15% }
          &:nth-child(5) { width: 15% }
          &:nth-child(6) { width: 15% }
          &:nth-child(7) { width: 15% }
          .rose-label {
            display: inline-block;
            width: 70px;
            height: 24px;
            text-align: center;
            line-height: 24px;
            padding: 0;
          }
          .evenSymbol {
            margin-left: 22px;
            font-weight: 700!important;
            cursor: pointer;
            font-size: 14px;
            position relative
            .coin-label {
              position absolute
              right 0
              transform translateX(100%)
              top 0
              white-space nowrap;
              position:absolute;
              right:-5px;
              transform:translateX(100%);
              top:-4px;
              white-space:nowrap;
              display:inline-block;
              font-size:12px;
              font-weight:400;
              padding:0px 4px;
              border-radius:2px;
              line-height:17px;
              overflow: hidden;
              .coin-label {
                position: relative;
                z-index: 1;
                margin: 0;
                padding: 0;
              }
              .coin-bg {
                display: inline-block;
                width: 100%;
                height: 100%;
                position: absolute;
                left: 0;
                top: 0;
                opacity: 0.2;
              }
            }
          }
          .marketIcon {
            cursor: pointer
            position: absolute;
          }
        }
        .newPrice {
          line-height: auto !important;
          padding-top: 14px;
          box-sizing: border-box;
          p {
            line-height: 15px;
          }
          .price {
            font-size: 14px;
          }
        }
        .even:first-child {
          text-align: left;
          position: relative
        }
      }
    }
  }
}
.tableContent {
  width: 1180px;
  margin: 50px auto 0 auto;
  position: relative;
  box-shadow: 0px -2px 10px rgba(0,0,0,0.2)
  .common-table {
    background: none;
  }
  .market-title-box {
    border-bottom-width: 1px;
    border-bottom-style: solid;
    height: 55px;
    line-height: 53px;
    padding-right: 320px;
    padding-left: 20px;
    border-radius: 2px 2px 0 0;
  }
  .market-title {
    display: flex;
    text-align: center;
    li {
      float: left;
      height: 53px;
      line-height: 55px;
      cursor: pointer;
      user-select: none;
      flex: 1;
      &.selected {
        border-bottom-width: 2px;
        border-bottom-style: solid;
      }
    }
  }
  .search {
    position: absolute;
    top: 12px;
    right: 20px;
    height: 28px;
    line-height: 26px;
    width: 200px;
    border-radius: 2px;
    border-width: 1px;
    border-style: solid;
    .icon-serach {
      margin: 0 10px;
    }
    .search-input {
      line-height: 26px;
      border: none;
      width: 165px;
    }
    input::-webkit-input-placeholder {
            color: #515A6B;
          }
  }

  .table-body td .rose-label {
    display: inline-block;
    width: 70px;
    height: 24px;
    text-align: center;
    line-height: 24px;
    padding: 0;
  }
  .table-body td .symbolName ,
  .table-body td .icon-btn {
    padding: 0;
    vertical-align: inherit;
    margin: 0;
    display: inline-block;
  }
  .table-body td .symbolName {
    width: 80%;
    margin-left: 10px;
  }
  .fontSize14 {
    font-size: 14px;
  }
}
