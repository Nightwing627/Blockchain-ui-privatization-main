.common-keyAuth {
  width: 100%;
  height: 100%;
  min-height: 500px;
  position: relative;
  .user-management-child-header {
    font-size: 14px;
    padding: 18px 0 18px 20px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    box-sizing: border-box;
  }
  .keyAuth-loadingBox {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate3d(-50%, 0, 0);
  }
  .keyAuth-body {
    width: 380px;
    margin-left 50%;
    padding: 40px 0;
    transform translateX(-50%)
  }
  .keyAuth-body-title {
    font-size: 14px;
  }
  .keyAuth-body-text {
    font-size: 12px;
    margin-top: 10px;
    margin-bottom: 20px;
  }
  .keyAuth-body-img {
    width: 100px;
    height: 100px;
    img {
      width: 100%;
      height: 100%;
      display: block;
    }
  }
  .keyAuth-body-time {
     font-size: 12px;
    margin-top: 10px;
    margin-bottom: 40px;
  }
}