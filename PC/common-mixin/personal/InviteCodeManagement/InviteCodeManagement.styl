.invite-code-management{
    width:100%;
    .header{
      width: 940px;
      margin:0 20px;
      border-bottom-style: solid;
      border-bottom-width: 1px;
      .title1{
        font-size: 22px;
        margin: 33px 0 10px 0;
      }
      .title2{
        font-size: 12px;
        margin-bottom: 22px;
      }
    }
    .center{
      width: 940px;
      padding: 0 20px;
      position: relative;
      border-bottom-style:solid;
      border-bottom-width:10px;
      .center-main{
        display: flex;
        margin-top: 22px;
        div:nth-of-type(1){
          margin-right: 160px;
        }
        div{
          p:nth-of-type(1){
            font-size: 12px;
            line-height: 22px;
          }
          p:nth-of-type(2){
            font-size: 16px;
          }
        }
      }
      .invitation-way{
        width: 100%;
        margin-top: 36px;
        p:nth-of-type(1){
          font-size:12px;
          margin-bottom:10px;
        }
        .invitation-ways{
          display: flex;
          margin-bottom: 20px;
          div{
            line-height: 56px;
            height:56px;
            svg:nth-of-type(1){
              margin:0 10px 0 16px;
            }
            svg:nth-of-type(2){
              margin:0 18px 0 12px;
              cursor:pointer;
            }
          }
          div:nth-of-type(1){
            width:180px;
            font-size: 14px;
            margin-right:10px;
          }
          div:nth-of-type(2){
            width 480px;
            height:56px;
            line-height: 56px;
            font-size: 12px;
            z-index: 2;
            svg:nth-of-type(3){
              cursor:pointer;
            }
            .inviteQECode-show{
              position: relative;
              svg{
                margin:0;
              }
              .center-img{
                display: none;
                position:absolute;
                top:18px;
                right:-72px;
                width:90px;
                height:90px;
                margin-right 0;
                z-index:2;
                img{
                  width:80px;
                  height:80px;
                  margin:5px;
                }
              }
            }
            .inviteQECode-show:hover{
              .center-img {
                display: block;
              }
            }
          }
        }
      }
      .center-bg{
        position: absolute;
        right:0;
        bottom: 0;
      }
    }
    .main-center{
      .main-table{
        width:100%;
        .main-table-title{
          font-size: 14px;
          padding:20px 0 20px 32px;
          border-bottom-style: solid;
          border-bottom-width: 1px;
        }
      }
    }
    .icon{
      cursor: pointer;
    }
  }