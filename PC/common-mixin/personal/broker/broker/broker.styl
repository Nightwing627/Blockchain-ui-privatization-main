.ex-broker {
  .broker-user-info{
    border-bottom-width: 10px;
    border-bottom-style: solid;
    height:164px;
    padding 0 30px;
    .invite-help{
      font-size: 14px;
      cursor pointer
      display: block;
      margin-left: -10px;
      // display inline-block;
    }
    .user-left {
      float: left;
    }
    .user-right {
      float: left;
      margin-top: 58px;
    }
    .user-box {
      display inline-block;
      margin-bottom: 20px;
    }
    .user-name{
      display inline-block;
      font-family: PingFangSC-Regular;
      letter-spacing: 0;
      padding 40px 48px 12px 0;
      .top{
        font-size: 12px;
        line-height: 16px;
        margin-bottom: 4px;
      }
      .phone{
        font-size: 22px;
        line-height: 30px;
      }
    }
    .user-text{
      padding: 0 16px;
      box-sizing: border-box;
      min-width: 220px;
      height: 40px;
      line-height: 40px;
      font-family: PingFangSC-Regular;
      display inline-block;
      margin-left: 10px;
      .title{
        text-align: left;
        font-size: 12px;
      }
      .info{
        float: right;
        font-family: PingFangSC-Medium;
        font-size: 22px;
      }
    }
  }
    // margin-top: -60px;
    .top-content {
      height: 299px;
      // padding-top: 60px;
      background-repeat: no-repeat;
      background-position: center bottom;
    }
    .warp {
      width: 100%;
      margin: 0 auto;
      .common-table {
        background: none !important;
        .lastTh div {
          margin-right: 10px;
        }
      }
    }
    .page-title {
      margin-top: 70px;
      text-align: center;
      font-size: 32px;
      line-height: 32px;
    }
    .f_r {
      float: right;
    }
    .my-minning-pct {
      margin-top: 96px;
      .tit {
        font-size: 12px;
        line-height: 12px;
      }
      .num {
        font-size: 14px;
        line-height: 14px;
      }
      .pct-bar {
        margin: 10px 0;
        overflow: hidden;
        height: 12px;
        .pct {
          height: 12px;
          width: 50%;
        }
      }
    }
    .wk-info {
      margin: 58px auto 0 auto;
      .item-box-left {
        margin-left: 450px;
      }
      .item-box-left.indireact {
        margin-left: 335px;
      }
      .item-box.userName{
        margin-left: 290px;
      }
      .item-box {
        min-width: 160px;
        float: left;
        margin-right: 40px;
        .box-bar {
          border-left-width: 1px;
          border-left-style: solid;
          padding-left: 20px;
        }
        .name {
          font-size: 32px;
          .dy {
            font-size: 14px;
          }
        }
        .txt {
          font-size: 12px;
          line-height: 20px;
          margin-top: 18px;
        }
        .padding {
          height: 44px;
        }
      }
    }
    .my-harvest {
      border-radius: 2px;
      height: 170px;
      .tit {
        font-size: 14px;
        color: #EDF4F8;
        padding: 16px 0 0 30px!important;
      }
      .sy-num {
        margin-top: 32px;
        padding-left: 40px;
        float: left;
        span {
          font-size: 32px;
          line-height: 32px;
          display: inline-block;
          margin-right: 5px;
        }
        b {
          font-size: 14px;
        }
        p {
          font-size: 12px;
          margin-top: 10px;
        }
      }
      .tt-num {
        float: right;
        margin-top: 40px;
        font-size: 14px;
        line-height: 14px;
        .tt-i {
          float: left;
          padding: 0 40px;
          line-height: 30px;
          &:last-child {
            border-left-width: 1px;
            border-left-style: solid;
          }
          .t {
            font-size: 12px;
            margin-right: 5px;
          }
        }
      }
    }
    .mining-data {
      margin-top: 0px!important;
    }
    .mining-tit {
      height: 56px!important;
      line-height: 54px!important;
      padding: 0 30px!important;
      border-bottom-width: 1px;
      border-bottom-style: solid;
      .item {
        display: inline-block;
        font-size: 14px;
        margin-right: 50px;
        height: 53px!important;;
        user-select: none;
        cursor: pointer;
        &.selected {
          border-bottom-width: 2px;
          border-bottom-style: solid;
        }
      }
      .search {
        float: right;
        line-height: 28px;
        margin-top: 14px;
        .common-button {
          float: left;
        }
      }
    }
    .filter-box {
      height: 65px;
      padding: 25px 20px 0 20px;
      .common-select, .input-line-content {
        float: left;
        margin-right: 20px;
      }
    }
  }
