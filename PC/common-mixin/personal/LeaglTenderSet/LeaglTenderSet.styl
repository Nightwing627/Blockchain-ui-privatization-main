.legal-tender-set {
  width 100%

  .user-management-child-header {
    font-size 14px
    padding 18px 0 18px 20px
    border-bottom-width 1px
    border-bottom-style solid
    box-sizing border-box
  }

  .user-management-main {
    width 100%
    float left

    .user-management-main ul li {
      width 100%
      float left
    }

    .li-icon {
      float left
      margin 25px 20px
    }

    .li-div {
      float left
      width 910px
      height 80px
      box-sizing border-box
      border-bottom-style solid
      border-bottom-width 1px
      font-size 14px
      position relative
    }

    .li-div:last-of-type(1) {
      border-bottom-style none
      border-bottom-width 0px
    }

    .li-div p:nth-of-type(1) {
      margin-top 20px
    }

    .li-divs p:nth-of-type(1) {
      line-height 24px
    }

    .li-div p:nth-of-type(1).switchBottom, .li-div p:nth-of-type(1).switchBottomChecked {
      margin-top 0
    }

    .li-div p:nth-of-type(1).none {
      margin-top 32px
    }

    .li-div p:nth-of-type(2) {
      margin-top 5px
      font-size 12px
      width 80%
    }

    .li-div-right {
      margin-left 64px
    }

    .li-title img {
      width 16px
      height 16px
      vertical-align middle
      margin-right 10px
    }

    .li-fun {
      float right
      font-size 12px
      cursor pointer
    }

    .li-right {
      position absolute
      top 32px
      right 21px
      float right

      .switchWrap {
        float right
        top 3px
        margin 0 6px
      }

      .common-button {
        top -5px
      }

      .user-button {
        font-size 12px
        margin-top -10px
      }
    }
  }

  .alertTitle {
    margin-bottom 15px
  }

  .alertText {
    width 285px
    font-size 12px
    margin-bottom 5px
    line-height 18px
  }

  .alertError {
    height 36px
    border-bottom-width 1px
    border-bottom-style solid
    line-height 36px

    .alertError-text {
      font-size 12px
    }

    .alertError-icon {
      float right
      margin-top 12px
    }
  }

  .alertBot {
    margin-bottom 10px
  }
}

.Int {
  .legal-tender-set .user-management-main .li-div {
    float none
    width auto
  }

  .legal-tender-set .user-management-main .li-icon {
    margin-left 0
  }

  .legal-tender-set .user-management-main .li-right {
    right 0
  }
}

.carp-coin {
  background rgba(#24A0F5, 0.15)
  border 0 solid #24A0F5
  border-radius 2px
  color #24A0F5
  padding 0 7px
  line-height 20px
  margin-left 8px
}