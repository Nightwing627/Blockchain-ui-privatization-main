.singPass-login-content {
  position: relative;
  padding: 30px 0;
  .account-login {
    position: absolute;
    cursor: pointer;
    right: 5px;
    top: 5px;
  }
  #qr_wrapper {
    width: 250px;
    height: 224px;
    margin: 0 auto;
    overflow: hidden;
    background: #fff;
    .spcpqr-c386.spcpqr-b3e6 .spcpqr-120f,
    .spcpqr-c386 .spcpqr-120f,
    .spcpqr-c386.spcpqr-b3e6 .spcpqr-9e90{
      display: none;
    }
    .spcpqr-c386.spcpqr-b3e6 .spcpqr-be88 {
      opacity: 1;
    }
    .spcpqr-c386 {
      padding: 30px 0;
    }
    .spcpqr-c386 .spcpqr-be88 .spcpqr-a5c3 {
      width: 50px;
    }
  }
  .spcpqr-cent {
    background: rgba(29, 38, 53, 0.8);
    position: absolute;
    width: 250px;
    height 224px;
    top: 30px;
    left 50%;
    transform: translateX(-50%);
    text-align: center;
    z-index: 9;
    p {
      margin: 100px 0 20px;
    }
    .common-button-slot {
      font-size: 12px;
    }
  }
  .sys {
    width: 250px;
    margin: 24px auto 0;
    .sys-icon {
      float: left;
      width: 32px;
      height 32px;

    }
    .sys-text {
      padding-left: 46px;
      width: 140px;
      line-height: 18px;
      font-size: 14px;
    }
  }
  .ass-item {
    height: 70px;
    line-height: 70px;
    padding-left: 30px;
    margin-top: 1px;
    cursor: pointer;
    border-radius: 3;
    &:hover {
      height: 68px;
      line-height: 68px;
      padding-left: 29px;
      border-width: 1px;
      border-style: solid;
    }
  }
}
.dow-singpass-app {
  text-align: right;
  margin: 20px 0;
  font-size: 12px;
  span {
    cursor: pointer;
  }
}
.selselogintype-dialog {
  color: #fff;
  .common-button {
    margin: 0;
  }
  .common-button-solid {
    display: none;
  }
}