.free-staking {
  .banner {
    width 100%
    background-position top center
    background-repeat no-repeat
    padding-top 1px

    &.header {
      height 400px
      box-sizing border-box
    }

    &.footer {
      padding 60px 0px
      background-image linear-gradient(180deg, rgba(10, 19, 32, 1) 30%, rgba(15, 41, 67, 1))
    }

    .banner-cont {
      width 1184px
      margin 0 auto
      position relative
      min-height: 240px
    }

    .article-cont {
      margin 276px auto 0 auto
      width 1184px
    }

    .banner-logo {
      position: absolute;
      display block
      width 380px
      height 240px
      top 50%
      left 0
      transform: translateY(-50%)
    }

    .rich-cont {
      float right
      width 680px
      box-sizing border-box

      .rich-title {
        font-size 40px
        line-height 56px
        margin 0 0 13px 0
        color rgba(237, 244, 248, 1)
      }

      .rich-text {
        color rgba(130, 142, 161, 1)
        font-size 14px
        line-height 22px
      }

      .rich-line {
        margin 15px 0px
        width 31px
        height 1px
        background-color rgba(32, 47, 71, 1)
      }

      .rich-faq, .rich-eamil {
        color rgba(130, 142, 161, 1)
        font-size 12px
        line-height 22px

        a {
          color rgba(36, 160, 245, 1)
        }
      }
    }
  }

  .free-staking-cont {
    margin 30px auto 0 auto
    width 1184px

    .title {
      .titlel {
        font-size 16px
        float left
      }

      .titler {
        font-size 14px
        float right
        cursor pointer

        span {
          vertical-align middle
        }

        .icon {
          vertical-align middle
          margin-right 8px
        }
      }
    }

    .nav-tab {
      padding-top 12px
      border-bottom 1px solid
    }

    .min-tab {
      margin-top 30px

      .min-tab-item {
        cursor pointer
        font-size 12px
        float left
        min-width 70px
        padding 0 10px
        height 24px
        line-height 24px
        text-align center
        border-radius 2px
        box-sizing border-box
      }
    }
  }

  .project-list {
    padding-bottom 160px
    width 100%
    margin-top 45px

    .project-item {
      border-radius 2px
      margin-top 20px
      width 100%
      height 169px

      .project-iteml {
        padding-left 31px
        float left
        margin 45px 0
        width 344px
        height 80px
        box-sizing border-box

        .logo {
          width 80px
          height 80px
          display block
          float left
        }

        .coin-base-info {
          width 233px
          margin-top 10px
          float left
          padding-left 30px
          height 67px
          border-right 1px solid
          box-sizing border-box

          .base-coin {
            text-align left
            font-family Helvetica-Bold
            font-size 32px
            padding 0px 0px 10px 0px
            position relative
            display inline-block

            .label-cont {
              position absolute
              transform translateX(100%)
              top 0px
              right -12px
              width 150px

              .label-type {
                &.hot {
                  background rgba(235, 77, 92, 0.15)
                }

                margin-right 12px
                vertical-align top
                text-align center
                font-family Roboto-Regular
                width 50px
                height 22px
                line-height 22px
                border-radius 0 6px 0 6px
                font-size 12px
                background rgba(36, 160, 245, 0.15)
                color #24A0F5
                display inline-block
              }
            }
          }

          .short-name {
            font-size 16px
            text-align left
            line-height 14px
          }
        }
      }

      .project-itemc {
        padding-left 48px
        margin-top 40px
        float left
        width 480px

        .project-title {
          padding-bottom 15px
          font-size 16px
          line-height 22px
        }

        .label {
          display inline-block
          float left
          margin-right 67px
          &:last-child{
            margin-right: 0px;
          }
        }

        .label-title {
          line-height 29px
          font-family Helvetica
          font-size 24px
          text-align left
          padding-bottom 6px
        }

        .label-value {
          line-height 17px
          font-size 12px
        }
      }

      .project-itemr {
        margin-top 65px
        float right
        padding-right 40px

        .project-status {
          float left
          line-height 40px

          .status-text {
            font-size 12px
            padding-right 17px
            padding-left 6px
          }
        }

        .nav-active {
          float left

          .nav-active-text {
            font-size 14px
          }
        }
      }
    }
  }

  .no-data {
    padding 200px 0px
    text-align center
  }
}