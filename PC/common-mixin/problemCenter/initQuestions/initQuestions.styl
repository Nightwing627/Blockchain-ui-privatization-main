.initQuestions {
  .title {
    line-height: 55px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    padding: 0 60px;
  }
  .content {
    width: 380px;
    padding-left: 60px;
    padding-top: 20px;
  }
  .input-line-warning {
    right: 0
  }
  .imgmk {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0.5;
    z-index: 1;
  }
  .uploadImg {
    overflow: hidden;
    cursor: pointer;
    width: 380px;
    height: 140px;
    margin-top: 20px;
    box-sizing: border-box;
    border-width: 1px;
    border-style: dashed;
    border-radius: 2px;
    position: relative;
  }
  .upload-image {
    width: 100%;
    height: 100%;
    margin-top: -18px;
    display: block;
  }
  .uploadMark {
    text-align: center;
    padding-top: 40px;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    z-index: 2;
  }
  .uploadMark-text {
    user-select: none
    font-size: 12px;
    margin-top: 10px;
  }
  .uploadMark-mess {
    user-select: none
    font-size: 12px;
    margin-top: 10px;
  }
  .uploadMark-input {
    width: 0;
    height: 0;
    opacity: 0;
  }
  .uploadLoading {
     z-index: 2;
    text-align: center;
    padding-top: 40px;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
  }
}