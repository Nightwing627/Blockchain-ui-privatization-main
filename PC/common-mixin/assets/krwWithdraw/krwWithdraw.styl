.page-withdraw {
  .addressBox-page-icon {
    line-height: 28px;
    cursor: pointer;
    position: relative;
    &:hover {
      .addressBox-page-market {
        display: block;
      }
    }
    .addressBox-page-market {
      display: none;
      position: absolute;
      top: 28px;
      width: 340px;
      padding: 20px;
      right: 0;
      box-sizing: border-box;
      font-family: PingFangSC-Regular;
      font-size: 12px;
      letter-spacing: 0;
      line-height: 20px;
      z-index: 4
    }
  }
  .options-message {
    margin-bottom: 17px;
    .key {
      font-size: 12px;
    }
    .value {
      font-size: 14px;
      margin-top: 6px;
    }
  }
  .withdraw-header {
    height: 55px;
    line-height: 55px;
    padding-left: 20px;
    font-size: 14px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
  }
  .goAddress {
    font-size: 12px;
    margin-left: 20px;
    cursor: pointer
  }
  .withdraw-details {
    padding: 28px 40px;
    border-bottom-width: 10px;
    border-bottom-style: solid;
    .messages {
      width: 380px;
      margin-top: 26px;
      font-size: 12px;
      .key {
        white-space: nowrap;
        text-align: right;
        padding: 0 8px;
        width: 1px;
      }
      .value {
        line-height: 16px;
        padding-right: 8px;
        position: relative;
        overflow: hidden;
        .valueInp {
          position: absolute;
          left: -1000px;
          top: -1000px;
        }
      }
      td {
        padding-top: 7px;
        padding-bottom: 7px;
        margin-top: 1px;
      }
      .copyTd {
        padding-right: 8px;
        text-align: right;
        .icon {
          cursor: pointer;
        }
        .svgs {
          .info {
            display: inline-block
          }
          .active {
            display: none
          }
          &:hover {
            .active {
              display: inline-block
            }
            .info {
              display: none
            }
          }
        }
      }
      .even {
        border-bottom-width: 2px;
        border-bottom-style: solid;
      }
    }
    .details-title {
      font-size: 14px;
      line-height: 12px;
      span { margin-right: 7px; }
    }
    .details-left {
      float: left;
    }
    .details-right {
      float: right;
      width: 380px;
      // 57-44=13  28-13=15
      margin-top: -15px
    }
    .symbol {
      font-size: 32px;
      line-height: 32px;
    }
    .numberList {
      margin-top: 22px;
      font-size: 12px;
      ul {
        float: left;
      }
      li {
        margin-bottom: 4px
      }
      .numberList-key {
        line-height: 20px;
        li { text-align: end; }
      }
      .numberList-value {
        line-height: 20px;
        margin-left: 20px;
      }
      .numberList-last {
        margin-left: 64px;
      }
    }
    .ServiceSpk {
      font-size: 12px;
      line-height: 30px;
      padding-right: 5px;
    }
    .details-warking {
      margin-top: 37px;
      font-size: 12px;
    }
  }
  .withdraw-table {
    width: 100%;
    box-sizing: border-box;
    padding: 0 20px;
    .table-head, .table-body-bar, .pagination{
      padding: 0!important;
    }
  }
}